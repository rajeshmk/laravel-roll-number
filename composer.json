{
    "name": "vocolabs/roll-number",
    "description": "The Vocolabs RollNumber package.",
    "license": "MIT",
    "authors": [
        {
            "name": "Raju Koyilandy",
            "email": "mkrajesh.kdy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0",
        "vocolabs/contracts": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "VocoLabs\\RollNumber\\": "src"
        },
        "files": [
            "helper-functions/voco-helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "VocoLabs\\RollNumber\\RollNumberServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
